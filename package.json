{
	"name": "money-calc",
	"version": "1.1.0",
	"description": "Helper class for calculating money amounts without rounding errors.",
	"author": "Adam Demasi <adam@chariz.com>",
	"contributors": [
		"Aarnav Tale <tale@chariz.com>"
	],
	"license": "Apache-2.0",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.mjs",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"build": "tsup ./src/index.ts --format cjs,esm --dts --clean",
		"prepublishOnly": "pnpm run build",
		"push": "np --no-tests"
	},
	"keywords": [
		"arithmetic",
		"calculate",
		"calculator",
		"currency",
		"decimal",
		"financial",
		"math",
		"maths",
		"money",
		"precise",
		"precision"
	],
	"engines": {
		"node": ">=16.6.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/chariz/money-calc.git"
	},
	"bugs": {
		"url": "https://github.com/chariz/money-calc/issues"
	},
	"homepage": "https://github.com/chariz/money-calc#readme",
	"dependencies": {
		"bson": "*",
		"js-big-decimal": "^1.3.4"
	},
	"devDependencies": {
		"@types/node": "^17.0.14",
		"np": "^7.6.0",
		"tsup": "^5.11.11",
		"typescript": "^4.4.3"
	}
}
