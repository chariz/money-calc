{
	"name": "money-calc",
	"version": "1.4.0",
	"description": "Helper class for calculating money amounts without rounding errors.",
	"author": "Adam Demasi <adam@chariz.com>",
	"contributors": [
		"Aarnav Tale <tale@chariz.com>"
	],
	"license": "Apache-2.0",
	"type": "module",
	"types": "./dist/index.d.ts",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.js",
			"import": "./dist/index.mjs"
		}
	},
	"scripts": {
		"build": "tsup ./src/index.ts --format cjs --target es5 --clean && tsup ./src/index.ts --format esm --target es2015 --dts",
		"lint": "eslint src/",
		"lint:fix": "eslint --fix src/",
		"prepublishOnly": "pnpm run build",
		"push": "np",
		"test": "pnpm -s lint && vitest run --coverage"
	},
	"keywords": [
		"arithmetic",
		"calculate",
		"calculator",
		"currency",
		"decimal",
		"financial",
		"math",
		"maths",
		"money",
		"precise",
		"precision"
	],
	"engines": {
		"node": ">=16.6.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/chariz/money-calc.git"
	},
	"bugs": {
		"url": "https://github.com/chariz/money-calc/issues"
	},
	"homepage": "https://github.com/chariz/money-calc#readme",
	"dependencies": {
		"bson": "^7.0.0",
		"js-big-decimal": "^2.2.0"
	},
	"devDependencies": {
		"@swc/core": "^1.15.3",
		"@types/node": "^24.10.1",
		"@vitest/coverage-v8": "4.0.15",
		"eslint": "^9.39.1",
		"eslint-config-chariz": "^2.1.0",
		"np": "^10.2.0",
		"tsup": "^8.5.1",
		"typescript": "^5.9.3",
		"vitest": "^4.0.15"
	}
}
